<!DOCTYPE html>
<head>
  <title>CheckOut</title>

  <link rel="stylesheet" href="/styles/checkout-page.css" type="text/css" />
  <link rel="stylesheet" href="/styles/header.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/items_to_orders.js"></script>
  <script type="text/javascript" src="/scripts/amount_with_qty.js"></script>

  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Domine&family=Noto+Sans:wght@200;300;400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
  </head>

  <body class="page-body">
    <%- include('partials/_header') %>
    <div id="my-background"></div>
    <div id="checkout-page">
    <form action="/checkout" class="placeorder" method="post">
      <h1>CHECK OUT</h1>
      <table class="itemsList">
      </table>
      <div class="receipt">
        <div class="total subtotals">
          <text>Subtotal</text>
          <span class="subtotal"></span>
        </div>
        <div class="total taxes">
          <text>Tax</text>
          <span class="tax"></span>
        </div>
        <div class="total totals">
          <text>Total</text>
          <span class="total_amount"></span>
        </div>
      </div>
      <button class="ordersubmit" type="submit">Place Order</button>
    </form>
    </div>
  </div>
  </body>
</html>
