<body class="page-body">
  <%- include('partials/_header') %>
  <div id="my-background"></div>
  <div id="menu-page">
  <main>
    <h1 class="main-page__greeting">Hi,
      <br>What is on your mind today?</h1>
    <section class="menu-list">
      <h2 class="visually-hidden">Menu list</h2>
    </section>
      <script src="menu-render.js"></script>
    </div>
    </div>

    <div class="menu">
      <h2>Menu</h2>
      <article action="/users/menu" method="POST">
  
        <h3>Appetizers</h3>
        <div class="appetizers">
        <label for="fries">fries <button type="submit" class="add" id="fries">Add</button></label>
        <label for="nuggets">chicken nuggets <button type="submit" class="add" id="chicken-nuggets">Add</button></label>
      </div>
        <h3>Meals</h3>
        <div class="meals">
        <label for="burger">burger combo <button type="submit" class="add" id="burgerC">Add</button></label>
        <label for="nuggets">chicken nuggets combo <button type="submit" class="add" id="chickenC">Add</button></label>
      </div>
        <h3>Drinks</h3>
        <div class="drinks">
        <label for="fries">coke <button type="submit" class="add" id="coke">Add</button></label>
        <label for="nuggets">sprite <button type="submit" class="add" id="sprite">Add</button></label>
      </div>
      </article>
      <script src="menu-render.js"></script>
    </div>

  </main>
  </div>
